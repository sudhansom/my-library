{
  "name": "sitemule-ng",
  "version": "0.1.0",
  "scripts": {
    "combodoc": "npx compodoc -p tsconfig.doc.json -s",
    "ng": "ng",
    "watch:ngx": "ng build ng-components --watch",
    "build:ngx:prod": "ng build ng-components --configuration production",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "docs": "compodoc -p ./tsconfig.json -s",
    "start": "npm run docs:json && start-storybook -p 6006",
    "build:storybook": "npm run docs:json && build-storybook --no-dll",
    "cy:test:storybook": "cypress run --headless -C cypress/cypress.storybook.json",
    "ci:start-and-test-storybook": "cross-env START_SERVER_AND_TEST_INSECURE=1 npm run start-server-and-test start:storybook http://localhost:6006 cy:test:storybook"
  },
  "scarfSettings": {
    "enabled": false
  },
  "private": true,
